{
  "name": "list-pagination",
  "version": "0.0.1",
  "description": "A slightly opinionated module for list pagination.",
  "main": "lib/index.js",
  "module": "lib/main.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint \"${INIT_CWD}/**/*.js\"",
    "pretest": "npm run lint",
    "test": "jest",
    "watch": "npm test -- --watch --coverage",
    "prepublishOnly": "npm test -- --coverage",
    "publish": "coveralls < coverage/lcov.info",
    "docs": "jsdoc2md -t docs/readme.hbs --files lib/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CentralPing/list-pagination"
  },
  "keywords": [
    "list",
    "pagination",
    "express",
    "middlware"
  ],
  "author": "Jason Cust <jason@centralping.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CentralPing/list-pagination/issues"
  },
  "homepage": "https://github.com/CentralPing/list-pagination",
  "dependencies": {
    "esm": "^3.0.37",
    "express-joi": "git+https://github.com/CentralPing/express-joi.git",
    "jsonwebtoken": "^8.2.1"
  },
  "peerDependencies": {
    "joi": "^13"
  },
  "devDependencies": {
    "body-parser": "^1.18.3",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-node": "^6.0.1",
    "express": "^4.16.3",
    "jest": "^23.0.0",
    "joi": "^13.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "supertest": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 100,
        "lines": 80,
        "statements": -10
      }
    },
    "testRegex": ".*spec\\.(unit|func|int)\\.js"
  }
}
